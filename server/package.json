{
  "name": "learn-express",
  "module": "src/app.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch ./src/app.ts",
    "build": "tsc -b",
    "start": "bun ./src/app.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --check . --write",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.21.1",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  }
}
